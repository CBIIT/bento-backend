FRONT_ENV=  #set this value to "dev" if you will be building the frontend container code locally, used for frontend development
FRONTEND_BUILD_DIR=  #the directory you are building into for local frontend development
BACKEND_IP=<required>  #the IP address used to access the backend webapp
BACK_ENV=  #set this value to "dev" if you will be building the backend container code locally, used for backend development
BACKEND_BUILD_DIR=  #the directory you are building into for local backend development
NEO4J_USER=<required>  #the user to set for Neo4j. The username is assumed to be the default "neo4j" if this is not set
NEO4J_PASS=<required>  #the password to set for Neo4j. The username is assumed to be the default "neo4j" if this is not set
