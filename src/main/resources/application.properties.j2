spring.mvc.throw-exception-if-no-handler-found=true
neo4j.graphql.endpoint.schema_endpoint=idl/
graphql.schema=graphql/{{schema_file}}
spring.mvc.view.prefix=/WEB-INF/
spring.mvc.view.suffix=.jsp
error.redirect_url=http://localhost/error.html
allow_graphql_query = true
allow_graphql_mutation =false
enable.redis={{enable_redis}}
redis.use_cluster=true
redis.port={{redis_port}}
redis.ttl=-1
bento.api.version={{bento_api_version}}
redis.filter.enable=true
redis.init.queries=graphql/redis-init-queries
redis.filter.queries=graphql/redis-filter-queries.yaml